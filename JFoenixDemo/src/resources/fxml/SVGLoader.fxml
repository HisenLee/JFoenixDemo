<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import com.jfoenix.controls.JFXButton?>
<StackPane fx:id="root" xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml">

    <VBox>
        <HBox>
            <padding>
                <Insets top="12" bottom="12" left="12" right="12"/>
            </padding>
            <JFXButton fx:id="browseFont" text="Browse SVG icomoon font">
            </JFXButton>

            <Label></Label>
        </HBox>
        <HBox VBox.vgrow="ALWAYS">
            <StackPane HBox.hgrow="ALWAYS" fx:id="iconsContainer"></StackPane>
            <StackPane fx:id="detailsContainer"></StackPane>
        </HBox>
    </VBox>

</StackPane>
